<h2>Resultados para '{{keyword}}' en {{market}}</h2>

{{#productos}}
    <div>
        <strong>{{name}}</strong> - {{price}} €
    </div>
{{/productos}}

{{^productos}}
    <p>No se encontraron productos.</p>
{{/productos}}

<h3>Filtrar por categoría:</h3>
<ul>
    {{#categorias}}
        <li><a href="{{url}}">{{name}}</a></li>
    {{/categorias}}
</ul>

<br><a href="/">Volver</a>
